  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["-c", "-d"]
    },
    {
      "name": "PowerShell: Create new named event with GCP",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["create", "-c", "-d", "-eventName builder", "-gcp"]
    },
    {
      "name": "PowerShell: Create new account and new event",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["create", "-c", "-d", "-eventName builder", "-newAccount asdf123"]
    },
    {
      "name": "PowerShell: Create Random new event",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["create", "-c", "-d"]
    },
    {
      "name": "PowerShell: Remove My Events",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["remove", "-c", "-d"]
    },
    {
      "name": "PowerShell: Danger! Delete all workshops older than 48h",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["remove", "-c", "-d", "-hourLimit 48"]
    },
    {
      "name": "PowerShell: Quick reload script",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["-c", "-d"]
    }
  ]
}
