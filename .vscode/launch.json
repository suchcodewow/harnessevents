  "version": "0.2.0",
  "configurations": [
    {
      "name": "PowerShell: Launch Current File",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["-c", "-v"]
    },
    {
      "name": "PowerShell: Create new named event",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["-c", "-v", "-headless", "-userCount 2", "-eventName eventbuilder", "-gcp"]
    },
    {
      "name": "PowerShell: Create Random new event",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["create", "-c", "-v"]
    },
    {
      "name": "PowerShell: Remove My Events",
      "type": "PowerShell",
      "request": "launch",
      "script": "${file}",
      "args": ["-janitorMode", "-c", "-v"]
    }
  ]
}
